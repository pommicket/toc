#include "std/io.toc";

total : int;

init ::= fn(x: int) {
	if #builtin("compiling") { return; }
	writes("Initializing... ");
	writei(x);
	puts("% complete");
	total += x;
}


main ::= fn() {
	writes("hello from main. total is ");
	puti(total);
}
