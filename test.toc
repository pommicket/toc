puti ::= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};
putf ::= fn(x: float) {
	 #C("printf(\"%f\\n\", (double)x);
");
};

arr ::= pkg "a";

putptri ::= fn(x: &int) {
		puti(*x);
};

main ::= fn() {
	 x : arr.Arr(int);
	 arr.arr_add(&x, 10);
	 arr.arr_add(&x, 20);
	 arr.arr_add(&x, 80);
	 arr.arr_add(&x, 5430);
	 arr.arr_add(&x, -6546);
	 arr.arr_foreach(x, putptri);
};