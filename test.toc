puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};

putch @= fn(x: char) {
	 #C("printf(\"%c\\n\", x);
");
};

putf @= fn(x: float) {
	 #C("printf(\"%f\\n\", (double)x);
");
};


N @= 50;
pascal @= fn() [N][]int {
	 x: [N][]int;
	 i := 0;
	 while i < N {
	 	   x[i] = new(int, i+1);
		   i = i + 1;
	 }
	 i = 0;
	 while i < N {
	 	   x[i][0] = 1;
		   x[i][i] = 1;
		   j := 1;
		   while j < i {
		   		 x[i][j] = x[i-1][j-1] + x[i-1][j];
				 j = j + 1;
		   }
		   i = i + 1;
	 }
	 x
};

foo @= fn() [3]int {
	x : [3]int;
	x[0] = 0;
	x[1] = 1;
	x[2] = 2;
	x
};

main @= fn() {
	 // ptriangle @= pascal();
	 // puti(ptriangle[49][25]);
	 x @= foo();
};	
