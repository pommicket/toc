puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};

☃ @= struct {
	x, y: int;
	z: float;
};
something @= fn() int {
		  5
};

bar @= fn(a := something()) f: ☃, g: int {
	f.x = a;
	f.y = 123;
	f.z += 43.2;
	g = 17;
};

main @= fn() {
f,g := bar();
puti(g);
puti(f.x);
puti(f.y);
h,i := bar();
puti(h.x);
};
