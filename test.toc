import ::= fn(x :: []char) &Package {
	p ::= pkg x;
	p_ptr ::= new(Package);
	*p_ptr = p;
	p_ptr
};

puts :: fn(&char) i32 = #foreign "puts", "libc.so.6";

hw ::= fn() int {
   s := "Hello, world!\0";
   puts(&s[0]);
   0
};

main ::= fn() {
	 // io ::= import("std/io");
	 // io.puts("Hello, world!");
	 hw();
	 x ::= hw();
};