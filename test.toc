arr ::= pkg "std/arr";
io ::= pkg "std/io";
main ::= fn() {
	x : arr.Arr(char);
	arr.arr_add(&x, 'H');
	arr.arr_add(&x, 'e');
	arr.arr_add(&x, 'l');
	arr.arr_add(&x, 'l');
	arr.arr_add(&x, 'o');
	arr.arr_add(&x, '!');

	s : [1]char;
	s[0] = (arr.arr_len(x) as char) + '0';
	io.puts(s[:]);
	for c := x.data {
		s[0] = c;
		io.puts(s[:]);
	}
};