f @= fn(t @ Type, x: t) t {
  x + 1
};

g @= fn(x, y @ Type) int {
(3 as x) + (5 as y)
};

main @= fn() {
	 puti(f(int, 3));
	 puti(f(u8, 255) as int);
	 puti(g(int, int));
};