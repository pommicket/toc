puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};

foo @= fn() int {
	x:= new(int, 10).len;
	x
};

bar @= fn() int {
	x := new(int,10);
	(&x).len
};

baz @= fn() int {
	x: [10]int;
	x.len
};

quux @= fn() int {
	 x: [10]int;
	 (&x).len
};

main @= fn() {
	 puti(foo());
	 X @= foo();
	 puti(X);
	 puti(bar());
	 Y @= bar();
	 puti(Y);
	 puti(baz());
	 Z @= baz();
	 puti(Z);
	 puti(quux());
	 W @= quux();
	 puti(W);
};
