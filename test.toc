factorial @= fn(x: int) int {
		  numbers := new(int, x);
		  i := 0;
		  while i < x {
		  numbers[i] = i+1;
		  i = i + 1;
		  }
		  product := 1;
		  i = 0;
		  while i < x{
		  product = product * numbers[i];
		  i = i + 1;
		  }
		  del(numbers);
		  product
};

