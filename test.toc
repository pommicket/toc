#include "std/io.toc", io;


s ::= struct (t :: Type, hasz ::= true) {
	x, y: t;
	#if hasz {
		z: t;
		a :: t = 3 as t;
	}
}

main ::= fn() {
	p: s(float);
	p.x = 7;
	p.y = 13;
	p.z = 12;
	io.puti(p.x as int);
	io.puti(s(int).a as int);
	q: s(int, false);
	q.x = 13;
	io.puti(q.x);
	//io.puti(q.a);
}

