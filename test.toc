puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};

total @= fn() int {
	  t := 0;
	 x := new(int,10);
	 i := 0;
	 while i < x.len {
		   x[i] = i;
		   i = i + 1;
	 }
	 
	 x.len = 5;
	 i = 0;
	 while i < x.len {
	 	   t = t + x[i];
		   i = i + 1;
	 }
	 t
};


main @= fn() {
	 puti(total());
	 X @= total();
	 puti(X);
};
