puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x)");
};

mktup @= fn(a: int, b: int) (int, int) {
	  a, a+b
};

asdf @= fn() int {
	 32187318
};

main @= fn() {
	 a, b := mktup(10, 20);
	 // x := mktup;
	 puti(asdf() as u8 as i64);
	 puti(a); puti(b);
	 x := 5+-if 5 { 6 } else { 7 };
	 puti(x);
};