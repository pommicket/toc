puti @= fn(x: int) {
	 #C("printf(\"%ld\\n\", (long)x);
");
};

putch @= fn(x: char) {
	 #C("printf(\"%c\\n\", x);
");
};

putf @= fn(x: float) {
	 #C("printf(\"%f\\n\", (double)x);
");
};

foo @= fn() [3]int {
	x : [3]int;
	x[0] =  1;
	x[1] = 2;
	x[2] = 3;
	x
};

getASDF @= fn(i: int) int {
		ASDF @= foo();
		ASDF[i]
};

main @= fn() {
	 // ptriangle @= pascal();
	 // puti(ptriangle[49][25]);
	 puti(getASDF(2));
	 puti(getASDF(1));
	 puti(getASDF(0));

};	
