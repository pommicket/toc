#include "std/mem.toc", mem;
#include "std/io.toc", io;
#include "std/base.toc", base;

main ::= fn() {
	file, _ := io.fopen_write("test.txt");
	io.writes("What is your name? ");
	b : [64]char;
	name := b[:];
	err := io.gets(&name, file);
	if err {
		base.error("File error");
	}
	io.writes("Hello, ");
	io.puts(name);
}
